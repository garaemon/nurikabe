(in-package #:nurikabe)

(defclass* <gl-material>
    ()
  ((name nil)
   (diffuse nil)
   (ambient nil)
   (specular nil)
   (emission nil)
   (shininess nil)
   (transparency nil)                   ;alpha = 1 - transparency
   ))

(defmethod print-object ((material <gl-material>) stream)
  (print-unreadable-object (material stream :type t :identity t)
    (format stream "~A" (name-of material))))

(defun make-gl-material (&key
                         (name nil)
                         (color (nh:double-vector 0.3 0.5 0.7 1))
                         (ambient (nh:scale 0.1d0 color))
                         (diffuse (nh:scale 1.0d0 color))
                         (specular (nh:scale 0.2d0 diffuse))
                         (emission (nh:scale 0.1d0 diffuse))
                         (shininess 20.0)
                         (transparency nil))
  (let ((diffuse (coerce diffuse 'cons))
        (ambient (coerce ambient 'cons))
        (specular (coerce specular 'cons))
        (emission (coerce emission 'cons)))
    ;; check length
    (if (= (length diffuse) 3)
        (setf diffuse (nconc diffuse (list 1.0d0))))
    (if (= (length ambient) 3)
        (setf ambient (nconc ambient (list 1.0d0))))
    (if (= (length specular) 3)
        (setf specular (nconc specular (list 1.0d0))))
    (if (= (length emission) 3)
        (setf emission (nconc emission (list 1.0d0))))
    (make-instance '<gl-material>
                   :name name
                   :diffuse diffuse
                   :ambient ambient
                   :specular specular
                   :emission emission
                   :shininess (if (listp shininess) shininess (list shininess))
                   :transparency transparency)))

(defmethod setup-material ((material <gl-material>))
  (with-slots (ambient diffuse specular emission shininess)
      material
    (gl:material-fv gl:+front-and-back+
                    gl:+ambient+
                    ambient)
    (gl:material-fv gl:+front-and-back+
                    gl:+diffuse+
                    diffuse)
    (gl:material-fv gl:+front-and-back+
                    gl:+specular+
                    specular)
    (gl:material-fv gl:+front-and-back+
                    gl:+emission+
                    emission)
    (gl:material-fv gl:+front-and-back+
                    gl:+shininess+
                    shininess)
    material))

